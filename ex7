#include <stdio.h>

int quantidade_valores_diferentes(int v[], int n) {
    int conjunto[1000] = {0};
    int quantidade = 0;

    for (int i = 0; i < n; i++) {
        if (conjunto[v[i]] == 0) {
            conjunto[v[i]] = 1; 
            quantidade++; 
        }
    }
    return quantidade;
}

int main() {
    int tamanho;
    printf("Digite o tamanho do vetor: ");
    scanf("%d", &tamanho);

    int vetor[tamanho];

    printf("Digite os %d elementos do vetor:\n", tamanho);
    for (int i = 0; i < tamanho; i++) {
        scanf("%d", &vetor[i]);
    }

    int qtde_valores_diferentes = quantidade_valores_diferentes(vetor, tamanho);

    printf("A quantidade de valores diferentes no vetor Ã©: %d\n", qtde_valores_diferentes);

    return 0;
}
