#include <stdio.h>

int main() {
    float a, b, c;

    printf("Digite o tamanho do lado a do triângulo: ");
    scanf("%f", &a);

    printf("Digite o tamanho do lado b do triângulo: ");
    scanf("%f", &b);

    printf("Digite o tamanho do lado c do triângulo: ");
    scanf("%f", &c);

    if (a < b + c && b < a + c && c < a + b) {
        printf("É possível formar um triângulo com os lados fornecidos.\n");
    } else {
        printf("Não é possível formar um triângulo com os lados fornecidos.\n");
    }

    return 0;
}
